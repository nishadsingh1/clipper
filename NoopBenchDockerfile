FROM clipper/py-rpc:latest
ARG model_name=bench_noop
ARG model_version=1

COPY containers/python/noop_container.py /containers/python/
COPY /bench/setup_noop_bench_docker.sh /bench/
COPY /bench/setup_bench_arg_exporter.sh /bench/
COPY /bench/export_aws_ip.sh /bench/

CMD ["/bench/setup_noop_bench_docker.sh $model_name $model_version"]

